  "dependencies": {
    "moment": "^2.29.1",
    "react": "17.0.1",
    "react-native": "0.64.0",
    "react-native-calendar-picker": "^7.1.1",
    "react-native-error-boundary": "^1.1.7",
    "react-native-masked-text": "^1.13.0",
